{
    "ext": ".js",
    "fileNameTemplate": "/{{entityName}}-repository",
    "technologies": [
        "node"
    ],
    "useCases": [
        {
            "name": "repository",
            "folder": "serverMiddleware/api/{{entityName}}/lib"
        }
    ],
    "entity": "entity.template",
    "contextComposite": "composite.template",
    "contexts": [
        "script"
    ],
    "generationStrategy": "singleAggregate",
    "primitives": [
        {
            "key": "entity",
            "type": "entity",
            "definition": "entity.template"
        }
    ]
}